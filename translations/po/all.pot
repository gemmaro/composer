#. type: Bullet: '- '
#: ../doc/04-schema.md:163
#, markdown-text
msgid "Apache-2.0"
msgstr ""

#. type: Bullet: '- '
#: ../doc/04-schema.md:163
#, markdown-text
msgid "BSD-2-Clause"
msgstr ""

#. type: Bullet: '- '
#: ../doc/04-schema.md:163
#, markdown-text
msgid "BSD-3-Clause"
msgstr ""

#. type: Bullet: '- '
#: ../doc/04-schema.md:163
#, markdown-text
msgid "BSD-4-Clause"
msgstr ""

#. type: Bullet: '- '
#: ../doc/04-schema.md:163
#, markdown-text
msgid "GPL-2.0-only / GPL-2.0-or-later"
msgstr ""

#. type: Bullet: '- '
#: ../doc/04-schema.md:163
#, markdown-text
msgid "GPL-3.0-only / GPL-3.0-or-later"
msgstr ""

#. type: Bullet: '- '
#: ../doc/04-schema.md:163
#, markdown-text
msgid "LGPL-2.1-only / LGPL-2.1-or-later"
msgstr ""

#. type: Bullet: '- '
#: ../doc/04-schema.md:163
#, markdown-text
msgid "LGPL-3.0-only / LGPL-3.0-or-later"
